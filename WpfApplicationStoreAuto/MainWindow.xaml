<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplicationStoreAuto"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" x:Class="WpfApplicationStoreAuto.MainWindow"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="Autogestion Tiendas" Height="800" Width="750" WindowStartupLocation="CenterScreen" Icon="/WpfApplicationStoreAuto;component/Resources/icon.png" Closing="Window_Closing">

    <Window.Resources>
        <ControlTemplate x:Key="ButtonIsEnable" TargetType="{x:Type Button}">
            <Border Background="WhiteSmoke" BorderBrush="Black" BorderThickness="1" 
        CornerRadius="0" x:Name="Border">
                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter TargetName="Border" Property="Background" Value="Gray" />
                </Trigger>
                <Trigger Property="IsEnabled" Value="True">
                    <Setter TargetName="Border" Property="Background" Value="WhiteSmoke" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
    </Window.Resources >

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TabControl x:Name="tabControl" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,0,0,15">
            <TabItem>
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Tareas Pendientes " FontWeight="Bold" FontSize="14"/>
                        <TextBlock x:Name="tabNumberPendingTask" Text="(0)" Foreground="DarkOrange" FontWeight="Bold" FontSize="14"/>
                        <TextBlock Text=" Tareas Aprobadas " FontWeight="Bold" FontSize="14"/>
                        <TextBlock x:Name="tabNumbeAprovedTask" Text="(0)" Foreground="Green" FontWeight="Bold" FontSize="14"/>
                        <TextBlock Text=" Tareas Denegadas " FontWeight="Bold" FontSize="14"/>
                        <TextBlock x:Name="tabNumberDeniedTask" Text="(0)" Foreground="Red" FontWeight="Bold" FontSize="14"/>
                        <!--<TextBlock Text=" Tareas Cerradas " FontWeight="Bold" FontSize="14"/>
                        <TextBlock x:Name="tabNumberClosedTask" Text="(0)" Foreground="DodgerBlue" FontWeight="Bold" FontSize="14"/>-->
                    </StackPanel>
                </TabItem.Header>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="100"/>
                        <RowDefinition Height="1*"/>
                    </Grid.RowDefinitions>

                    <Border BorderBrush="Black" BorderThickness="0 0 0 3">
                        <Grid Grid.Row="0" Margin="5">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="180"/>
                                <ColumnDefinition Width="1*"/>
                            </Grid.ColumnDefinitions>
                            <Label Grid.Row="0" Grid.Column="0" Margin="0,2,0,2" Content="Navegación Rápida:"/>
                            <ComboBox x:Name="comboBoxQuickNavigation" IsReadOnly="true" Margin="4,4,4,4" Grid.Row="0" Grid.Column="1" ItemsSource="{Binding}" DropDownClosed="comboBoxQuickNavigation_DropDownClosed">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding DisplayValue}"/>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                            <Label Grid.Row="1" Grid.Column="0" Margin="0,2,0,2" Content="Navegación Rápida Aprobadas:"/>
                            <ComboBox x:Name="comboBoxQuickNavigationAprove" IsReadOnly="true" Margin="4,4,4,4" Grid.Row="1" Grid.Column="1" ItemsSource="{Binding}" DropDownClosed="comboBoxQuickNavigationAprove_DropDownClosed">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding DisplayValue}"/>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                            <Label Grid.Row="2" Grid.Column="0" Margin="0,2,0,2" Content="Navegación Rápida Denegadas:"/>
                            <ComboBox x:Name="comboBoxQuickNavigationrDenied" IsReadOnly="true" Margin="4,4,4,4" Grid.Row="2" Grid.Column="1" ItemsSource="{Binding}" DropDownClosed="comboBoxQuickNavigationrDenied_DropDownClosed">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding DisplayValue}"/>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                        </Grid>
                    </Border>

                    <ScrollViewer x:Name="scrollViewer" Grid.Row="1" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Visible" Margin="0,5,0,-5" ScrollChanged="scrollViewer_ScrollChanged">
                        <StackPanel x:Name="stackPanel" VerticalAlignment="Stretch" Margin="0,16,0,0">
                            <GroupBox x:Name="groupBox" Tag="1">
                                <GroupBox.Header>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" FlowDirection="LeftToRight">
                                        <TextBlock Text="Tarea 1" Tag="1" FontWeight="Bold" Foreground="Red"/>
                                        <TextBlock Text=" - Desbloquear Usuarios"  FontWeight="Bold"/>
                                    </StackPanel>
                                </GroupBox.Header>
                                <StackPanel Margin="0" Orientation="Vertical" Height="125" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="LightBlue">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="28"/>
                                            <ColumnDefinition Width="92"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>

                                        <Label Content="Nombre:" Grid.Row="0" Grid.Column="0" Margin="0,2,0,2" Grid.ColumnSpan="2"/>
                                        <ComboBox x:Name="comboBoxUnblockUser" IsReadOnly="true" Margin="4,4,4,4" Grid.Row="0" Grid.Column="2"/>
                                        <Label Content="Estatus de solicitud:" Margin="0,2,0,2" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2"/>
                                        <Label x:Name="labelStatusUnblock"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="1" Grid.Column="2"/>
                                        <Label Content="Mensaje de Soporte:" Margin="0,2,0,2" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2"/>
                                        <Label x:Name="labelMessageUnblock"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="2" Grid.Column="2"/>
                                    </Grid>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="35"/>
                                        </Grid.RowDefinitions>
                                        <Button x:Name="buttonSolicitudeUnblockUser" Template="{StaticResource ButtonIsEnable}" Content="Solicitar" HorizontalAlignment="Left" Margin="4,0,0,5" VerticalAlignment="Bottom" Width="75"  Height="30"  ToolTip="Solicitar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonSolicitudeUnblock_Click"/>
                                        <Button x:Name="buttonExecuteUnblockUser" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar" IsEnabled="false" HorizontalAlignment="Left" Margin="90,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Ejecutar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteUnblock_Click"/>
                                        <Button x:Name="buttonExecuteUnblockUserOffline" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar Offline" IsEnabled="True" HorizontalAlignment="Right" Margin="0,0,4,0"  VerticalAlignment="Top" Width="110"  Height="30"  ToolTip="Ejecutar Tarea Offline"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteUnblockUserOffline_Click"/>
                                    </Grid>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="groupBox2" Tag="2">
                                <GroupBox.Header>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" FlowDirection="LeftToRight">
                                        <TextBlock Text="Tarea 2" Tag="2" FontWeight="Bold" Foreground="Red"/>
                                        <TextBlock Text=" - Desbloquear Impresora Fiscal" FontWeight="Bold"/>
                                    </StackPanel>
                                </GroupBox.Header>
                                <StackPanel Margin="0" Orientation="Vertical" Height="125" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="White">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="120"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>

                                        <Label Content="Caja:" Grid.Row="0" Grid.Column="0" Margin="0,2,0,2"/>
                                        <ComboBox x:Name="comboBoxUnblockFiscalMachine" IsReadOnly="true" Margin="4,4,4,4" Grid.Row="0" Grid.Column="1" ItemsSource="{Binding}">
                                            <ComboBox.ItemTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding DisplayValue}"/>
                                                </DataTemplate>
                                            </ComboBox.ItemTemplate>
                                        </ComboBox>
                                        <Label Content="Estatus de solicitud:" Margin="0,2,0,2" Grid.Row="1" Grid.Column="0"/>
                                        <Label x:Name="labelStatusUnblockFiscalMachine"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="1" Grid.Column="1"/>
                                        <Label Content="Mensaje de Soporte:" Margin="0,2,0,2" Grid.Row="2" Grid.Column="0"/>
                                        <Label x:Name="labelMessageUnblockFiscalMachine"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="2" Grid.Column="1"/>
                                    </Grid>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="35"/>
                                        </Grid.RowDefinitions>
                                        <Button x:Name="buttonSolicitudeUnblockFiscalMachine" Template="{StaticResource ButtonIsEnable}" Content="Solicitar" HorizontalAlignment="Left" Margin="4,0,0,5" VerticalAlignment="Bottom" Width="75"  Height="30"  ToolTip="Solicitar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonSolicitudeUnblockFiscalMachine_Click"/>
                                        <Button x:Name="buttonExecuteUnblockFiscalMachine" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar" IsEnabled="false" HorizontalAlignment="Left" Margin="90,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Ejecutar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteUnblockFiscalMachine_Click"/>
                                        <Button x:Name="buttonExecuteUnblockFiscalMachineOffline" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar Offline" IsEnabled="True" HorizontalAlignment="Right" Margin="0,0,4,0"  VerticalAlignment="Top" Width="110"  Height="30"  ToolTip="Ejecutar Tarea Offline"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteUnblockFiscalMachineOffline_Click"/>
                                    </Grid>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="groupBox3" Tag="3">
                                <GroupBox.Header>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" FlowDirection="LeftToRight">
                                        <TextBlock Text="Tarea 3" Tag="3" FontWeight="Bold" Foreground="Red"/>
                                        <TextBlock Text=" - Reenvio de Cierre" FontWeight="Bold"/>
                                    </StackPanel>
                                </GroupBox.Header>
                                <StackPanel Margin="0" Orientation="Vertical" Height="125" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="LightBlue">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="120"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>

                                        <Label Content="Fecha:" Grid.Row="0" Grid.Column="0" Margin="0,2,0,2"/>
                                        <DatePicker x:Name="datePickerResendCloser" Margin="4,4,4,4" Grid.Row="0" Grid.Column="1" SelectedDate="{x:Static sys:DateTime.Now}" Focusable="False"/>
                                        <Label Content="Estatus de solicitud:" Margin="0,2,0,2" Grid.Row="1" Grid.Column="0"/>
                                        <Label x:Name="labelStatusResendCloser"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="1" Grid.Column="1"/>
                                        <Label Content="Mensaje de Soporte:" Margin="0,2,0,2" Grid.Row="2" Grid.Column="0"/>
                                        <Label x:Name="labelMessageResendCloser"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="2" Grid.Column="1"/>
                                    </Grid>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="35"/>
                                        </Grid.RowDefinitions>
                                        <Button x:Name="buttonSolicitudeResendCloser" Template="{StaticResource ButtonIsEnable}" Content="Solicitar" HorizontalAlignment="Left" Margin="4,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Solicitar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonSolicitudeResendCloser_Click"/>
                                        <Button x:Name="buttonExecuteResendCloser" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar" IsEnabled="false" HorizontalAlignment="Left" Margin="90,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Ejecutar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteResendCloser_Click"/>
                                    </Grid>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="groupBox4" Tag="4">
                                <GroupBox.Header>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" FlowDirection="LeftToRight">
                                        <TextBlock Text="Tarea 4" Tag="4" FontWeight="Bold" Foreground="Red"/>
                                        <TextBlock Text=" - Cerrar caja (cuando se asigno mas no se uso)" FontWeight="Bold"/>
                                    </StackPanel>
                                </GroupBox.Header>
                                <StackPanel Margin="0" Orientation="Vertical" Height="155" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="White">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="120"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>

                                        <Label Content="Caja:" Grid.Row="0" Grid.Column="0" Margin="0,2,0,2"/>
                                        <ComboBox x:Name="comboBoxCloseCashMachine" IsReadOnly="true" Margin="4,4,4,4" Grid.Row="0" Grid.Column="1" ItemsSource="{Binding}">
                                            <ComboBox.ItemTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding DisplayValue}"/>
                                                </DataTemplate>
                                            </ComboBox.ItemTemplate>
                                        </ComboBox>
                                        <Label Content="Fecha:" Grid.Row="1" Grid.Column="0" Margin="0,2,0,2"/>
                                        <DatePicker x:Name="datePickerCloseCashMachine" Margin="4,4,4,4" Grid.Row="1" Grid.Column="1" SelectedDate="{x:Static sys:DateTime.Now}" Focusable="False"/>
                                        <Label Content="Estatus de solicitud:" Margin="0,2,0,2" Grid.Row="2" Grid.Column="0"/>
                                        <Label x:Name="labelStatusCloseCashMachine"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="2" Grid.Column="1"/>
                                        <Label Content="Mensaje de Soporte:" Margin="0,2,0,2" Grid.Row="3" Grid.Column="0"/>
                                        <Label x:Name="labelMessageCloseCashMachine"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="3" Grid.Column="1"/>
                                    </Grid>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="35"/>
                                        </Grid.RowDefinitions>
                                        <Button x:Name="buttonSolicitudeCloseCashMachine" Template="{StaticResource ButtonIsEnable}" Content="Solicitar" HorizontalAlignment="Left" Margin="4,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Solicitar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonSolicitudeCloseCashMachine_Click"/>
                                        <Button x:Name="buttonExecuteCloseCashMachine" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar" IsEnabled="false" HorizontalAlignment="Left" Margin="90,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Ejecutar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteCloseCashMachine_Click"/>
                                        <Button x:Name="buttonExecuteCloseCashMachineOffline" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar Offline" IsEnabled="True" HorizontalAlignment="Right" Margin="0,0,4,0"  VerticalAlignment="Top" Width="110"  Height="30"  ToolTip="Ejecutar Tarea Offline"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteCloseCashMachineOffline_Click"/>
                                    </Grid>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="groupBox5" Tag="5">
                                <GroupBox.Header>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" FlowDirection="LeftToRight">
                                        <TextBlock Text="Tarea 5" Tag="5" FontWeight="Bold" Foreground="Red"/>
                                        <TextBlock Text=" - Igualar Neto Fiscal (cuando neto de ventas tiene un monto y neto fiscal esta en 0)" FontWeight="Bold"/>
                                    </StackPanel>

                                </GroupBox.Header>
                                <StackPanel Margin="0" Orientation="Vertical" Height="155" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="LightBlue">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="120"/>
                                            <ColumnDefinition Width="40*"/>
                                            <ColumnDefinition Width="547*"/>
                                        </Grid.ColumnDefinitions>

                                        <Label Content="Caja:" Grid.Row="0" Grid.Column="0" Margin="0,2,0,2"/>
                                        <ComboBox x:Name="comboBoxEqualsFiscalNeto" IsReadOnly="true" Margin="4,4,4,4" Grid.Row="0" Grid.Column="1" ItemsSource="{Binding}" Grid.ColumnSpan="2">
                                            <ComboBox.ItemTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding DisplayValue}"/>
                                                </DataTemplate>
                                            </ComboBox.ItemTemplate>
                                        </ComboBox>
                                        <Label Content="Fecha:" Grid.Row="1" Grid.Column="0" Margin="0,2,0,2"/>
                                        <DatePicker x:Name="datePickerEqualsFiscalNeto" Margin="4,4,4,4" Grid.Row="1" Grid.Column="1" SelectedDate="{x:Static sys:DateTime.Now}" Focusable="False" Grid.ColumnSpan="2"/>
                                        <Label Content="Estatus de solicitud:" Margin="0,2,0,2" Grid.Row="2" Grid.Column="0"/>
                                        <Label x:Name="labelStatusEqualsFiscalNeto"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2"/>
                                        <Label Content="Mensaje de Soporte:" Margin="0,2,0,2" Grid.Row="3" Grid.Column="0"/>
                                        <Label x:Name="labelMessageEqualsFiscalNeto"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2"/>
                                    </Grid>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="35"/>
                                        </Grid.RowDefinitions>
                                        <Button x:Name="buttonSolicitudeEqualsFiscalNeto" Template="{StaticResource ButtonIsEnable}" Content="Solicitar" HorizontalAlignment="Left" Margin="4,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Solicitar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonSolicitudeEqualsFiscalNeto_Click"/>
                                        <Button x:Name="buttonExecuteEqualsFiscalNeto" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar" IsEnabled="false" HorizontalAlignment="Left" Margin="90,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Ejecutar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteEqualsFiscalNeto_Click"/>
                                    </Grid>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="groupBox6" Tag="6">
                                <GroupBox.Header>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" FlowDirection="LeftToRight">
                                        <TextBlock Text="Tarea 6" Tag="6" FontWeight="Bold" Foreground="Red"/>
                                        <TextBlock Text=" - Actualizar monto caja chica" FontWeight="Bold"/>
                                    </StackPanel>
                                </GroupBox.Header>
                                <StackPanel Margin="0" Orientation="Vertical" Height="125" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="White">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="120"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>

                                        <Label Content="Monto:" Grid.Row="0" Grid.Column="0" Margin="0,2,0,2"/>
                                        <xctk:DecimalUpDown x:Name="decimalUpDownUpdateAmountPettyCashFund" Grid.Row="0" Grid.Column="1" Margin="4,4,4,4" VerticalAlignment="Top" HorizontalAlignment="Stretch" Minimum="0" Maximum="100000000" FormatString="F" Value="0" KeyDown="upDownUpdate_KeyDown"/>
                                        <Label Content="Estatus de solicitud:" Margin="0,2,0,2" Grid.Row="1" Grid.Column="0"/>
                                        <Label x:Name="labelStatusUpdateAmountPettyCashFund"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="1" Grid.Column="1"/>
                                        <Label Content="Mensaje de Soporte:" Margin="0,2,0,2" Grid.Row="2" Grid.Column="0"/>
                                        <Label x:Name="labelMessageUpdateAmountPettyCashFund"  Margin="0,2,0,2" Content="----------" FontWeight="Bold" Grid.Row="2" Grid.Column="1"/>

                                    </Grid>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="35"/>
                                        </Grid.RowDefinitions>
                                        <Button x:Name="buttonSolicitudeUpdateAmountPettyCashFund" Template="{StaticResource ButtonIsEnable}" Content="Solicitar" HorizontalAlignment="Left" Margin="4,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Solicitar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonSolicitudeUpdateAmountPettyCashFund_Click"/>
                                        <Button x:Name="buttonExecuteUpdateAmountPettyCashFund" Template="{StaticResource ButtonIsEnable}" Content="Ejecutar" IsEnabled="false" HorizontalAlignment="Left" Margin="90,0,0,0" VerticalAlignment="Top" Width="75"  Height="30"  ToolTip="Ejecutar Tarea"
                                    Background="WhiteSmoke"
                                    Foreground="Black"
                                    FontFamily="Arial"
                                    FontWeight="Bold" Click="buttonExecuteUpdateAmountPettyCashFund_Click"/>
                                    </Grid>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="groupBox7" Tag="7">
                                <GroupBox.Header>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" FlowDirection="LeftToRight">
                                        <TextBlock Text="Tarea 7" Tag="7" FontWeight="Bold" Foreground="Red"/>
                                        <TextBlock Text=" - Actualizar tasa Euro" FontWeight="Bold"/>
                                    </StackPanel>
                                </GroupBox.Header>
                                <StackPanel Margin="0" Orientation="Vertical" Height="125" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="LightBlue">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="120"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>

                                        <Label Content="Monto:" Grid.Row="0" Grid.Column="0" Margin="0,2,0,2"/>
                                        <xctk:DecimalUpDown x:Name="decimalUpDownUpdateEuro" Grid.Row="0" Grid.Column="1" Margin="4,4,4,4" VerticalAlignment="Top" HorizontalAlignment="Stretch" Minimum="0" Maximum="100000000" FormatString="F" Value="0" KeyDown="upDownUpdate_KeyDown"/>
                                        <Label Content="Estatus de solicitud:" Margin="0,2,0,2" Grid.Row="1" Grid.Column="0"/>
